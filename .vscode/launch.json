    "version": "0.2.0",
    "configurations": [
        {
            "command": "npm run dev",
            "name": "dev",
            "request": "launch",
            "type": "node-terminal",
            "console": "integratedTerminal",
            "envFile": "${workspaceFolder}/.env"
        },
        {
            "command": "npm run start",
            "name": "start",
            "request": "launch",
            "type": "node-terminal",
            "console": "integratedTerminal",
            "envFile": "${workspaceFolder}/.env"
         },
         {
            "command": "npm run test:unit",
            "name": "test",
            "request": "launch",
            "type": "node-terminal",
            "console": "integratedTerminal",
            "envFile": "${workspaceFolder}/.env"
         },
         {
            "type": "node",
            "request": "attach",
            "name": "Docker",
            "port": 9226,
            "address": "10.177.50.103",
            "protocol": "inspector",
            "localRoot": "${workspaceFolder}/src",
            "remoteRoot": "/usr/src"
         },
         {
            "type": "node",
            "request": "attach",
            "name": "Docker: local",
            "port": 9226,
            "address": "localhost",
            "protocol": "inspector",
            "localRoot": "${workspaceFolder}/src",
            "remoteRoot": "/usr/src"
         }
    ]
}